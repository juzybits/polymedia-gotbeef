{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [ "^build" ],
            "outputs": [ "dist/**" ]
        },
        "clean": {
            "cache": false,
            "outputs": []
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "lint": {
            "outputs": []
        },
        "test": {
            "dependsOn": [ "^build" ],
            "outputs": []
        }
    }
}
